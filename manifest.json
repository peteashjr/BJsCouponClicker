{
    "name": "BJs Coupon Clicker",
    "version": "1.0",
    "description": "Clicking all available BJs coupons",
    "manifest_version": 2,
	"background": {
		"scripts": ["jquery.min.js", "BJsScript.js", "background.js"],
		"persistent": false
	},
	"browser_action": {},
	"permissions": [
		"activeTab"
    ],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png" 
	}
 }